<aura:component implements="flexipage:availableForAllPageTypes,force:appHostable,forceCommunity:availableForAllPageTypes">

    <!-- Set the Twitter Username / Handle for the timeline feed -->
    <aura:attribute name="title" type="String" default="Tweets"  />
    <aura:attribute name="username" type="String" default="salesforce" />  
    
    <div>
        <div class="timeline-wrapper">
            <div class="timeline-title"> 
            	<h3>{!v.title} </h3>
            </div>
            <div class="iframe-wrap">
                <iframe class="theframe" src="{!$SfdcSite.pathPrefix + '/apex/TMcTwitterFeedVF?username=' + v.username}" />
            </div>
        </div>
    </div>

</aura:component>